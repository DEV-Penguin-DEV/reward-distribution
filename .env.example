# Database credentials
DB_USER=example_user            # Username for the PostgreSQL database
DB_PASSWORD=example_password    # Password for the PostgreSQL database
DB_NAME=example_database        # Database name
HOST=localhost                  # Database host, usually localhost in development
DB_PORT=5432                    # Port on which PostgreSQL is running, default is 5432

# Telegram bot information
TELEGRAM_TOKEN=insert_your_telegram_bot_token_here     # Telegram bot token obtained from BotFather
TELEGRAM_CHAT_ID=insert_your_telegram_chat_id_here     # Telegram chat ID where the bot should send messages

# Infura project details for Ethereum network interaction
INFURA_PROJECT_ID=insert_your_infura_project_id_here   # Project ID from Infura for accessing Ethereum network

# Ethereum Contract details
CONTRACT_ADDRESS=insert_your_contract_address_here     # Address of the Ethereum contract you are interacting with
CONTRACT_ABI_PATH=/path_to_your_contract_abi.json      # Local path to the ABI file of your Ethereum contract
